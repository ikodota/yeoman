{
    "name": "ikodota/yeoman",
    "description": "yeoman is a friendly administrator management system written in Laravel 5.3.",
    "keywords": ["yeoman", "admin", "management", "laravel", "role", "permission", "setting", "logviewer"],
    "homepage": "https://github.com/ikodota/yeoman",
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/ikodota/yeoman/issues",
        "source": "https://github.com/ikodota/yeoman"
    },
    "authors": [
        {
            "name": "ikodota",
            "email": "ikodota@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "league/fractal": "^0.14.0",
        "ikodota/upload":"1.0.*",
        "owen-it/laravel-auditing":"^3.1",
        "rap2hpoutre/laravel-log-viewer": "^0.7.0"
    },
    "require-dev": {
        "symfony/dom-crawler": "3.1.*",
        "barryvdh/laravel-ide-helper": "^2.2",
        "barryvdh/laravel-debugbar": "^2.3",
        "prettus/l5-repository": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "Ikodota\\Yeoman\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
